{
  "name": "volto-v2-solucoes",
  "version": "1.0.0-alpha.0",
  "description": "Site da V2 Soluções",
  "main": "src/index.ts",
  "license": "MIT",
  "keywords": [
    "volto-addon",
    "volto",
    "plone",
    "react"
  ],
  "author": "Luis Flavio Rocha",
  "homepage": "https://github.com/lflrocha/v2-solucoes#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:lflrocha/v2-solucoes"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "i18n": "rm -rf build/messages && NODE_ENV=production i18n --addon",
    "dry-release": "release-it --dry-run",
    "release": "release-it",
    "release-major-alpha": "release-it major --preRelease=alpha",
    "release-alpha": "release-it --preRelease=alpha",
    "lint:fix": "stylelint 'packages/**/src/**/*.{css,scss,less}' --fix && prettier --write 'packages/**/src/**/*.{js,jsx,ts,tsx,css,scss,less}'"
  },
  "addons": [
    "@eeacms/volto-accordion-block",
    "@kitconcept/volto-slider-block",
    "@kitconcept/volto-social-blocks",
    "@kitconcept/volto-separator-block",
    "@mbarde/volto-image-crop-widget",
    "volto-form-block",
    "@kitconcept/volto-button-block",
    "@plone-collective/volto-authomatic",
    "@eeacms/volto-timeline-block",
    "@kitconcept/volto-light-theme"
  ],
  "theme": "",
  "dependencies": {
    "@plone/components": "^3.0.2",
    "@plone/registry": "^3.0.0-alpha.8",
    "@plone-collective/volto-authomatic": "3.0.0-alpha.4",
    "@kitconcept/volto-social-blocks": "1.0.0-alpha.8",
    "@mbarde/volto-image-crop-widget": "0.5.1",
    "@kitconcept/volto-light-theme": "7.0.0-alpha.25",
    "@eeacms/volto-timeline-block": "*",
    "volto-form-block": "*"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@plone/registry": "^3.0.0-alpha.8",
    "@plone/scripts": "^3.10.3",
    "@plone/types": "^1.4.5",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "typescript": "^5.7.3",
    "release-it": "^19.0.5"
  }

}
